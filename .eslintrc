{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true,
    "node": true

  },
  "extends": [
    "plugin:react/recommended"
    ],
  "parserOptions": {
     "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error"
  }
}